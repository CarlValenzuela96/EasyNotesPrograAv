<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="clases.RamoAprobado" table="ramoAprobado" lazy="false">
		<id name="idRamoAprobado" column="idRamoAprobado" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nombreRamo" column="nombreRamo" type="string" length="50" not-null="false" lazy="false"/>
		<property name="notaAprobacion" column="notaAprobacion" type="double" not-null="false" lazy="false"/>
		<many-to-one name="historialidHistorial" cascade="lock" column="HistorialidHistorial" class="clases.Historial" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
